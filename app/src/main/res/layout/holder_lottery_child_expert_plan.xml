<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/dp_10"
    android:background="@color/white"
    android:orientation="vertical"
    android:paddingStart="@dimen/dp_15"
    android:paddingEnd="@dimen/dp_0"
    android:paddingBottom="@dimen/dp_10">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <com.facebook.drawee.view.SimpleDraweeView
            android:id="@+id/expertPlanAvatar"
            android:layout_width="@dimen/dp_45"
            android:layout_height="@dimen/dp_45"
            android:layout_marginTop="@dimen/dp_15"
            android:contentDescription="@string/app_name"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:placeholderImage="@mipmap/ic_base_user"
            app:placeholderImageScaleType="centerCrop"
            app:roundAsCircle="true" />

        <TextView
            android:id="@+id/tvExpertPlanName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_15"
            android:textColor="@color/color_333333"
            android:textSize="@dimen/sp_16"
            android:textStyle="bold"
            app:layout_constraintStart_toEndOf="@+id/expertPlanAvatar"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="不懂温世书" />

        <TextView
            android:id="@+id/rtvExpertWinPresent"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_15"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:background="@drawable/color_blue"
            android:gravity="center"
            android:paddingStart="@dimen/dp_7"
            android:paddingEnd="@dimen/dp_7"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_9"
            app:layout_constraintStart_toEndOf="@+id/expertPlanAvatar"
            app:layout_constraintTop_toBottomOf="@+id/tvExpertPlanName"
            tools:text="重庆时时彩" />

        <TextView
            android:id="@+id/rtvExpertWinAdd"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_15"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_11"
            android:background="@drawable/color_orange"
            android:gravity="center"
            android:paddingStart="@dimen/dp_7"
            android:paddingEnd="@dimen/dp_7"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_9"
            app:layout_constraintStart_toEndOf="@+id/rtvExpertWinPresent"
            app:layout_constraintTop_toBottomOf="@+id/tvExpertPlanName"
            tools:text="胜率 70%" />

        <TextView
            android:id="@+id/rtvExpertWinProfit"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_15"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_marginTop="@dimen/dp_11"
            android:background="@drawable/color_yellow"
            android:gravity="center"
            android:paddingStart="@dimen/dp_7"
            android:paddingEnd="@dimen/dp_7"
            android:textColor="@color/white"
            android:textSize="@dimen/sp_9"
            app:layout_constraintStart_toEndOf="@+id/rtvExpertWinAdd"
            app:layout_constraintTop_toBottomOf="@+id/tvExpertPlanName"
            tools:text="3 连红" />


        <TextView
            android:id="@+id/tvExpertIssue"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_15"
            android:text="@string/home_expert_percent"
            android:textColor="@color/color_999999"
            android:textSize="@dimen/sp_12"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            tools:text="200111111111" />


        <com.fenghuang.baselib.widget.round.RoundTextView
            android:id="@+id/tvExpertPlanType"
            android:layout_width="wrap_content"
            android:paddingStart="@dimen/dp_7"
            android:paddingEnd="@dimen/dp_7"
            android:layout_height="18dp"
            android:layout_marginTop="@dimen/dp_13"
            android:gravity="center"
            android:text="@string/home_recommend_ten"
            android:textColor="@color/color_FF513E"
            android:textSize="@dimen/sp_10"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/expertPlanAvatar"
            app:rv_backgroundColor="@color/color_FFECE8"
            app:rv_cornerRadius="@dimen/dp_9" />

        <TextView
            android:id="@+id/tvTenList"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/dp_24"
            android:layout_marginTop="@dimen/dp_14"
            android:orientation="horizontal"
            android:textSize="@dimen/sp_12"
            android:textStyle="bold"
            android:textColor="@color/color_333333"
            app:layout_constraintStart_toEndOf="@+id/tvExpertPlanType"
            app:layout_constraintTop_toBottomOf="@+id/expertPlanAvatar"
            tools:text="1    3    5    6" />


        <com.fenghuang.baselib.widget.round.RoundLinearLayout
            android:id="@+id/linExpertPlanRecentlyTen"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_28"
            android:layout_marginTop="@dimen/dp_10"
            android:orientation="horizontal"
            android:paddingStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10"
            android:paddingEnd="@dimen/dp_10"
            app:layout_constraintTop_toBottomOf="@+id/tvExpertPlanType"
            app:rv_backgroundColor="@color/color_F5F7FA"
            app:rv_cornerRadius="@dimen/dp_5">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center"
                android:text="@string/lottery_recently_plan"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />

            <TextView
                android:id="@+id/tvCode_0"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_marginStart="@dimen/dp_5"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_1"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_2"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_3"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_4"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_5"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_6"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_7"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_8"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
            <TextView
                android:id="@+id/tvCode_9"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="center"
                tools:text="胜"
                android:textColor="@color/color_999999"
                android:textSize="@dimen/sp_12" />
        </com.fenghuang.baselib.widget.round.RoundLinearLayout>
    </androidx.constraintlayout.widget.ConstraintLayout>

</RelativeLayout>