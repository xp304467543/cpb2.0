<?xml version="1.0" encoding="utf-8"?>
<com.fenghuang.baselib.widget.round.RoundLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dp_188"
    android:gravity="center_horizontal"
    app:rv_backgroundColor="@color/white"
    android:orientation="vertical"
    app:rv_cornerRadius="@dimen/dp10">

    <TextView
        android:id="@+id/tvMsgSu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="@dimen/sp_16"
        android:layout_marginTop="@dimen/dp_15"
        android:textColor="@color/color_333333"
        android:textStyle="bold"
        android:text="申请成功"/>

    <TextView
        android:id="@+id/tvMsgContent"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/dp_40"
        android:textSize="@dimen/sp_14"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginEnd="@dimen/dp_15"
        android:layout_marginTop="@dimen/dp_15"
        android:textColor="@color/color_999999"
        android:gravity="center"
        android:text=""/>
    
    <Button
        android:id="@+id/btSuccessSure"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dp_40"
        android:layout_marginStart="@dimen/dp_35"
        android:layout_marginEnd="@dimen/dp_35"
        android:layout_marginTop="@dimen/dp_30"
        android:text="@string/sure"
        android:textColor="@color/white"
        android:background="@drawable/button_background"/>

</com.fenghuang.baselib.widget.round.RoundLinearLayout>